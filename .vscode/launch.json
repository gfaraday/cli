  "version": "0.2.0",
  "configurations": [
    {
      "name": "faraday_cli tag",
      "type": "dart",
      "request": "launch",
      "program": "bin/faraday.dart",
      "args": [
        "tag",
        "--project",
        "/Users/gix/Documents/yxr/gfraday/faraday_reborn",
        "-v",
        "1.0.7+5",
        "--platforms",
        "ios",
      ]
    },
    {
      "name": "faraday_cli completion",
      "type": "dart",
      "request": "launch",
      "program": "bin/faraday.dart",
      "args": [
        "completion",
        "--offset",
        "420",
        "--source-code",
        "",
        "--verbose"
      ]
    },
    {
      "name": "faraday_cli generate",
      "request": "launch",
      "program": "bin/faraday.dart",
      "args": [
        "generate",
        "--android-common",
        "/Users/gix/Documents/yxr/androidtob/app/src/main/java/faraday/bridge/FaradayCommon.kt",
        // "--ios-common",
        // "/Users/gix/Documents/yxr/iosb4reborn/YuxiaorReborn/Faraday/FaradayCommon.swift",
        "--file",
        "/Users/gix/Documents/yxr/gfraday/faraday_reborn/lib/src/features/ui.dart"
      ],
      "type": "dart"
    },
    {
      "name": "faraday_cli init",
      "request": "launch",
      "program": "bin/faraday.dart",
      "args": [
        "init",
        "--project-path",
        "../../example"
      ],
      "type": "dart"
    }
  ]
}
      "name": "faraday_cli tag",
      "type": "dart",
      "request": "launch",
      "program": "bin/faraday.dart",
      "args": [
        "tag",
        "--project",
        "/Users/gix/Documents/yxr/gfraday/faraday_reborn",
        "-v",
        "1.0.0+5",
        "--platforms",
        "ios",
      ]
    },
    {
      "name": "faraday_cli completion",
      "type": "dart",
      "request": "launch",
      "program": "bin/faraday.dart",
      "args": [
        "completion",
        "--offset",
        "191",
        "--source-code",
        "",
        "--verbose"

      ]
    },
    {
      "name": "faraday_cli generate",
      "request": "launch",
